<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <style name="ActionSheetDialogBase">
        <item name="com.android.actionsheetdialog:titleTextColor">#d4000000</item>
        <item name="com.android.actionsheetdialog:titleTextSize">20dp</item>
        <item name="com.android.actionsheetdialog:titleHeight">42dp</item>
        <item name="com.android.actionsheetdialog:messageTextColor">#73000000</item>
        <item name="com.android.actionsheetdialog:messageTextSize">12sp</item>
        <item name="com.android.actionsheetdialog:messageHeight">42dp</item>
        <item name="com.android.actionsheetdialog:itemTextColor">#00beb4</item>
        <item name="com.android.actionsheetdialog:itemTextSize">15sp</item>
        <item name="com.android.actionsheetdialog:itemHeight">48dp</item>
        <item name="com.android.actionsheetdialog:positiveTextColor">#de2b2b</item>
        <item name="com.android.actionsheetdialog:positiveTextSize">15sp</item>
        <item name="com.android.actionsheetdialog:positiveHeight">48dp</item>
        <item name="com.android.actionsheetdialog:cancelTextColor">#00beb4</item>
        <item name="com.android.actionsheetdialog:cancelTextSize">15sp</item>
        <item name="com.android.actionsheetdialog:cancelHeight">48dp</item>
        <item name="com.android.actionsheetdialog:cancelTopMargins">8dp</item>
        <item name="com.android.actionsheetdialog:cancelBackground">@drawable/radios</item>
        <item name="com.android.actionsheetdialog:sheetMargins">8dp</item>
        <item name="com.android.actionsheetdialog:contentBackground">@drawable/radios</item>
    </style>


    
    <style name="ActionSheetTextBase">
        <item name="android:gravity">center</item>
    </style>

    <style name="ActionSheetTextBase.Title">

        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#d4000000</item>
    </style>
    <style name="ActionSheetTextBase.Message">
        <item name="android:minHeight">42dp</item>
        <item name="android:textColor">#73000000</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="ActionSheetTextBase.Content">

    </style>
    <style name="ActionSheetTextBase.Cancel">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#00beb4</item>
        <item name="android:text">取消</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:background">@drawable/radios</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>
</resources>